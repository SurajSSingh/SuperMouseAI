  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "next-patch": "deno run -A npm:tauri-version patch --no-git && git-cliff -o CHANGELOG.md",
    "next-minor": "deno run -A npm:tauri-version minor --no-git && git-cliff -o CHANGELOG.md",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "full-check": "echo 'Type check:' && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && echo 'Format check:' && deno fmt --check && echo 'Lint check:' && deno lint .",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tauri": "tauri",
    "rebind": "echo \"Refreshing ./src/lib/bindings.ts\"; deno task tauri dev --no-dev-server-wait --no-dev-server --no-watch -f export-bindings-only"
  },
  "license": "FSL-1.1-MIT OR FSL-1.1-ALv2",
