{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main",
    "overlay"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "notification:default",
    "clipboard-manager:allow-write-text",
    "store:default",
    "log:default",
    "sentry:default",
    "process:default",
    "http:default",
    "fs:default",
    "dialog:default"
  ]
}