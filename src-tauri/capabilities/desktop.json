{
  "identifier": "desktop-capability",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "windows": [
    "main",
    "overlay"
  ],
  "permissions": [
    "global-shortcut:default",
    "global-shortcut:allow-is-registered",
    "global-shortcut:allow-register",
    "global-shortcut:allow-unregister",
    "global-shortcut:allow-unregister-all",
    "notification:default",
    "clipboard-manager:allow-write-text",
    "store:default",
    "log:default",
    "sentry:default",
    "process:default",
    "updater:default",
    "fs:default",
    "dialog:default",
    "fs:scope-applocaldata",
    "fs:scope-applocaldata-recursive",
    "fs:scope-applocaldata-index",
    {
      "identifier": "fs:allow-exists",
      "allow": [
        {
          "path": "$APPLOCALDATA"
        },
        {
          "path": "$APPLOCALDATA/models"
        },
        {
          "path": "$APPLOCALDATA/models/*"
        }
      ]
    },
    {
      "identifier": "fs:allow-create",
      "allow": [
        {
          "path": "$APPLOCALDATA"
        },
        {
          "path": "$APPLOCALDATA/models"
        },
        {
          "path": "$APPLOCALDATA/models/*"
        }
      ]
    },
    {
      "identifier": "fs:allow-write",
      "allow": [
        {
          "path": "$APPLOCALDATA"
        },
        {
          "path": "$APPLOCALDATA/models"
        },
        {
          "path": "$APPLOCALDATA/models/*"
        }
      ]
    },
    {
      "identifier": "fs:allow-write-file",
      "allow": [
        {
          "path": "$APPLOCALDATA"
        },
        {
          "path": "$APPLOCALDATA/models"
        },
        {
          "path": "$APPLOCALDATA/models/*"
        }
      ]
    },
    {
      "identifier": "fs:allow-remove",
      "allow": [
        {
          "path": "$APPLOCALDATA"
        },
        {
          "path": "$APPLOCALDATA/models"
        },
        {
          "path": "$APPLOCALDATA/models/*"
        }
      ]
    },
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://github.com/SurajSSingh/SuperMouseAI/*"
        },
        {
          "url": "https://huggingface.co/ggerganov/whisper.cpp/resolve/main/*"
        }
      ],
      "deny": [
        {
          "url": "https://private.tauri.app"
        }
      ]
    },
    "upload:default"
  ]
}